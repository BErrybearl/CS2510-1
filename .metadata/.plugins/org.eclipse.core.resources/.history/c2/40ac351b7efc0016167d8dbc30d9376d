import java.util.*;

class Course {
    String name;
    Instructor prof;
    ArrayList<Student> students;

    Course(String name, Instructor prof) {
        this.name = name;
        this.prof = prof;
        this.students = new ArrayList<Student>();
        prof.addCourse(this);
    }

    void addStudent(Student s) {
        this.students.add(s);
    }
}

class Instructor {
    String name;
    ArrayList<Course> courses;

    Instructor(String name) {
        this.name = name;
        this.courses = new ArrayList<Course>();
    }

    void addCourse(Course c) {
        courses.add(c);
    }

    boolean dejavu(Student s) {
        int numOfClasses = 0;
        for (Course c : this.courses) {
            if (c.students.contains(s)) {
                numOfClasses += 1;
            }
        }
        return numOfClasses > 1;
    }
}

class Student {
    String name;
    int id;
    ArrayList<Course> courses;

    Student(String name, int id) {
        this.name = name;
        this.id = id;
        this.courses = new ArrayList<Course>();
    }

    void enroll(Course c) {
        this.courses.add(c);
        c.addStudent(this);
    }

    boolean classmates(Student s) {
        for (Course c : this.courses) {
            if (c.students.contains(s)) {
                return true;
            }
        }
        return false;
    }
}